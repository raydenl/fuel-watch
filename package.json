{
  "name": "my-new-project",
  "main": "node_modules/expo/AppEntry.js",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest.js --watchAll",
    "lint": "tslint src/**/*.ts",
    "tsc": "tsc",
    "clean": "rimraf build",
    "copy": "copyfiles -u 1 src/assets/**/* build",
    "build": "npm run clean && npm run copy && npm run tsc --",
    "watch": "npm run build -- -w",
    "watchAndRunAndroid": "concurrently \"npm run watch\" \"npm run android\"",
    "buildRunAndroid": "npm run build && npm run watchAndRunAndroid ",
    "watchAndRunIOS": "concurrently \"npm run watch\" \"npm run ios\"",
    "buildRunIOS": "npm run build && npm run watchAndRunIOS ",
    "watchAndStart": "concurrently \"npm run watch\" \"npm run start\"",
    "buildAndStart": "npm run build && npm run watchAndStart "
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^10.0.2",
    "debounce": "^1.2.0",
    "expo": "^46.0.14",
    "firebase": "^6.2.2",
    "react": "16.8.6",
    "react-native": "https://github.com/expo/react-native/archive/sdk-33.0.0.tar.gz",
    "react-native-dialog": "^5.6.0",
    "react-native-loading-spinner-overlay": "^1.0.1",
    "react-native-masked-text": "^1.12.3",
    "react-native-sentry": "^0.43.2",
    "react-navigation": "^3.11.0",
    "react-redux": "^7.1.0",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "sentry-expo": "^1.13.0",
    "throttle-debounce": "^2.1.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-react-jsx-source": "^7.2.0",
    "@types/debounce": "^3.0.0",
    "@types/expo": "^32.0.13",
    "@types/expo__vector-icons": "^9.0.1",
    "@types/firebase": "^3.2.1",
    "@types/immutable": "^3.8.7",
    "@types/jest": "^24.0.15",
    "@types/react": "^16.8.22",
    "@types/react-dom": "^16.8.4",
    "@types/react-native": "^0.57.63",
    "@types/react-native-dialog": "^5.5.0",
    "@types/react-native-loading-spinner-overlay": "^0.5.1",
    "@types/react-navigation": "^3.0.7",
    "@types/react-redux": "^7.1.0",
    "@types/react-test-renderer": "^16.8.2",
    "@types/throttle-debounce": "^1.1.1",
    "babel-preset-expo": "^5.2.0",
    "concurrently": "^4.1.0",
    "copyfiles": "^2.1.0",
    "jest-expo": "33.0.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.2"
  }
}
